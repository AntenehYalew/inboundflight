<body>
<div class="maincontainer">
    <%if(loggedUser){%>
    <nav class="navbar bg-navbar">
        <div class="container">
                <a class="navbar-brand" href="/stationpage">Home</a>  
                <a class="" href="/logout"><i class="fas fa-sign-out-alt"></i> <%=loggedUser%></a>         
        </div>
      </nav>
      <div class="flash-div">
        <% if(error && error.length>0){ %>
          <div class="alert alert-danger flashmessage" role="alert">
            <h6><%= error%></h6>
          </div>
       <% }else if (success && success.length>0){%>
        <div class="alert alert-info flashmessage" role="alert">
          <h6> <%= success%></h6>
        </div>
       <%}%>
      </div>

    <% }%>

   
  


